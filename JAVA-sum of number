import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
    int n,sum=0;
        Scanner s=new Scanner(System.in);
       n=s.nextInt();
    while(n!=-1)
    {
        sum=sum+n;
      n=s.nextInt();
    }
    System.out.printf("%d",sum);

        
    }
}
