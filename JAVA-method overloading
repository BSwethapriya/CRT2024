class Test
{
 void add(int a,int b)
 {
  System.out.println("2 int args");
 }
 void add(int a,int b,int c)
 {
    System.out.println("3 int args");
 }
 void add(double a,double b)
 {
    System.out.println("2 double args");
 }
public static void main(String args[])
{
Test t1=new Test();
t1.add(12,33,45);
t1.add(23.5,67.8);
t1.add(78,22);
}
}

