import java.util.*;
class Building
{
  public static void main(String args[])
  {
   int n;
   Scanner s=new Scanner(System.in);
   n=s.nextInt();
   int []a=new int[n];
   for(int i=0;i<n;i++)
   {
    a[i]=s.nextInt();
   }
   int count=1;
   int lar=a[0];
   for(int i=0;i<n;i++)
   {
    if(lar<a[i])
    {
     count++;
    }
   }
   System.out.println(count);
}
}
