class Student
{
  int admno=111;
  String name="sss";
  public static void main(String ags[])
  {
   Student s1=new Student();
   s1.admno=101;
   s1.name="sss";
   System.out.println(s1.admno+" "+s1.name);
   Student s2=new Student();
   s2.admno=102;
   s2.name="ppp";
   System.out.println(s2.admno+" "+s2.name);
  }
} 
