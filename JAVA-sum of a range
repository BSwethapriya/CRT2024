import java.io.*;
import java.util.*;

public class Solution
{
    public static void main(String[] args)
    {
     int min,max,i,sum=0;
     Scanner s=new Scanner(System.in);
     min=s.nextInt();
     max=s.nextInt();
   for(i=min;i<=max;i++)
       sum=sum+i;
    {
       System.out.printf("%d",sum);
    }
    }
    
}
