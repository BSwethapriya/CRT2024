class Test
{
 void add(int a,int b,int c)
 {
  System.out.println("3 int args");
 }
 void add(float a,float b)
 {
    System.out.println("2 float args");
 }
 
public static void main(String args[])
{
Test t1=new Test();
t1.add(12,33,45);
t1.add(23.5f,67.8f);
t1.add(78,22);
t1.add('A','B');
t1.add(34.5f,67.8f);
}
}

