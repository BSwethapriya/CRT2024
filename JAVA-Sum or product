import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
     
     int r,n,m;
     Scanner s=new Scanner(System.in);
        n=s.nextInt();
        m=s.nextInt();
     r=n*m;
    if(r>1000)
    {
        System.out.printf("%d",n+m);
    }
    else
    {
        System.out.printf("%d",r);
    }
    }
}
